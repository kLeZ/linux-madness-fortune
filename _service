    <param name="version">1.0</param>
    <param name="filename">linux_madness</param>
  </service>
  <service name="recompress" mode="enabled">
    <param name="file">linux_madness.tar</param>
    <param name="compression">gz</param>
  </service>
  <service name="set_version" mode="disabled"/>
</services>
